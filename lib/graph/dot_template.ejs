<%_ function HtmlId(id) {
  return 'HREF="remove_me_url" ID="' + id + '"';
} %>
<%_ function TEXT(str) {
  if (str === '')
    return '';
  str = str.replace(/]/, '&#93;');
  return '<FONT>' + str + '</FONT>';
} %>
digraph {
  graph [
    rankdir = "LR"
  ];
  node [
    fontsize = "16"
    fontname = "helvetica, open-sans"
    shape = "plaintext"
  ];
  edge [
  ];
  ranksep = 2.0
<%_ _.each(typeGraph, node => { -%>
  "<%- node.name %>" [
    id = "<%- node.id %>"
    label = <<TABLE ALIGN="LEFT" BORDER="0" CELLBORDER="1" CELLSPACING="0" CELLPADDING="5">
      <TR>
        <%_ var kindLabel = node.kind !== 'OBJECT' ? '<<' + node.kind.toLowerCase() + '>>' : '' -%>
        <TD <%- HtmlId(`TYPE_TITLE::${node.name}`) %>
          ><B><%- node.name %></B><BR/><%= kindLabel %></TD>
      </TR>
      <%_ _.each(node.fields, field => { -%>
      <TR>
        <TD <%- HtmlId(field.id) %>
          ALIGN="LEFT" PORT="<%- field.name %>">
          <TABLE CELLPADDING="0" CELLSPACING="0" BORDER="0">
            <TR>
              <%_ var [left, right] = stringifyWrappers(field.typeWrappers); -%>
              <TD ALIGN="LEFT"><%- field.name %><FONT>  </FONT></TD>
              <TD ALIGN="RIGHT"><%- TEXT(left) %><%- field.type.name %><%- TEXT(right) %></TD>
            </TR>
          </TABLE>
        </TD>
      </TR>
      <%_ }); -%>
      <%_ _.each(node.possibleTypes, possibleType => { -%>
      <TR>
        <TD <%- HtmlId(possibleType.id) %>
          ALIGN="LEFT" PORT="<%- possibleType.type.name %>"><%- possibleType.type.name %></TD>
      </TR>
      <%_ }); -%>
      <%_ _.each(node.derivedTypes, derivedType => { -%>
      <TR>
        <TD <%- HtmlId(derivedType.id) %>
          ALIGN="LEFT" PORT="<%- derivedType.type.name %>"><%- derivedType.type.name %></TD>
      </TR>
      <%_ }); -%>
    </TABLE>>
  ];
  <%_ _(node.fields).each(field => { -%>
  <%_ if (!typeGraph[field.type.id]) return; -%>
  "<%- node.name %>":"<%- field.name %>" -> "<%- field.type.name %>" [
    id = "EDGE::<%- field.id %>"
    label = "<%- node.name %>:<%- field.name %>"
  ]
  <%_ }); -%>
  <%_ _(node.possibleTypes).each(possibleType => { -%>
  "<%- node.name %>":"<%- possibleType.type.name %>" -> "<%- possibleType.type.name %>" [
    id = "EDGE::<%- possibleType.id %>"
    style = "dashed"
  ]
  <%_ }); -%>
  <%_ _(node.derivedTypes).each(derivedType => { -%>
  "<%- node.name %>":"<%- derivedType.type.name %>" -> "<%- derivedType.type.name %>" [
    id = "EDGE::<%- derivedType.id %>"
    style = "dotted"
  ]
  <%_ }); -%>
<%_ }); -%>
}
